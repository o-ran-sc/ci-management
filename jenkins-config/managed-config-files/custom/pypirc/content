# .pypirc for O-RAN-SC Jenkins prodution configuration
# To use in sandbox, remove the [pypi] section

[distutils] # this tells distutils what package indexes you can push to
index-servers =
  $PYPIRC_URL_USR

[$PYPIRC_URL_USR]
repository: $PYPIRC_URL_PSW
username: $PYPIRC_USR
password: $PYPIRC_PSW
